<h1>New Song</h1>
<%= form_for(@song) do |s| %>
  <%= s.label :name, id: "song[name]" %><br>
  <%= s.text_field :name, id: "song[name]" %><br>
  <%= s.label :artist_id %><br>
  <%= s.text_field :artist_id %><br>
  <%= s.label :genre_id %><br>
  <%= s.text_field :genre_id %><br>
  <%= s.submit %>
<% end %>
